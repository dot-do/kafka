{
  "name": "kafka.do",
  "type": "module",
  "version": "0.0.4",
  "description": "Kafka-compatible streaming platform on Cloudflare Workers with Durable Object SQLite",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "import": "./dist/errors.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "author": "Nathan Clevenger",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/kafka.git"
  },
  "homepage": "https://github.com/dot-do/kafka#readme",
  "bugs": {
    "url": "https://github.com/dot-do/kafka/issues"
  },
  "keywords": [
    "kafka",
    "streaming",
    "cloudflare",
    "workers",
    "durable-objects",
    "event-streaming",
    "message-queue",
    "pub-sub",
    "queue"
  ],
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "prepublishOnly": "npm run build && vitest --run"
  },
  "dependencies": {
    "hono": "^4.6.0"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.11.1",
    "@cloudflare/workers-types": "^4.20241127.0",
    "typescript": "^5.7.0",
    "vitest": "^3.2.4",
    "wrangler": "^4.54.0"
  }
}
